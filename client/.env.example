# Example environment variables for deployment
# Copy this file to .env.local for local development or configure in your deployment platform

# Backend API URL for production deployment
# For Vercel/Netlify: use your deployed backend URL
# For local development: http://localhost:5000/api
EXPO_PUBLIC_API_URL=https://your-backend-domain.com/api

# Or use relative URL if frontend and backend are deployed together
# EXPO_PUBLIC_API_URL=/api
